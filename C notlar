    printf("ikinci toplam %.1f\n",c+d);
    printf("ikinci cikarma: %.1f",c-d);
    return 0;
}

*/

int main()
{
    int sayi1,sayi2,sonuc,cikarma;
    float rakam1,rakam2,sonuc2,cikarma2;
    
    

    printf("1. sayiyi gir: ");
    scanf("%d",&sayi1);
    
    printf("2. sayiyi gir: ");
    scanf("%d",&sayi2);
    
    printf("1. rakami gir: ");
    scanf("%f",&rakam1);
    
    printf("2. rakami gir: ");
    scanf("%f",&rakam2);
    
    sonuc=sayi1+sayi2;
    cikarma=sayi1-sayi2;
    sonuc2=rakam1+rakam2;
    cikarma2=rakam1-rakam2;
    
    printf("\n\n");
    printf("ilk toplam: %d\n",sonuc);
    printf("ilk cikarma: %d\n",cikarma);
    printf("ikinci toplam %.1f\n",sonuc2);
    printf("ikinci cikarma: %.1f",cikarma2);
    return 0;
}

