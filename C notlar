#include <stdio.h>
#include <stdlib.h>

// STRUCTERS
//ÖNCE YUKARIYA DEĞİŞKENLERİ ATA CHAR CHAR İNT FLOAT DİYE SONRA AŞAĞIYA 
// STRUCT ORNEK DENEME={..} SONRA PRİNTF(ADİ: %S,DENEME.İSİM)

struct ornek
{
	char isim[10];
	char soyisim[10];
	char takim[11];
	int yas;
};


int main() {
	
	struct ornek deneme={"Burak","Guler","Galatasaray",23};
	
	printf("Adiniz: %s\n",deneme.isim);
	printf("Soyadiniz: %s\n",deneme.soyisim);
	printf("Tuttugunuz takim: %s\n",deneme.takim);
	printf("Yasiniz: %d",deneme.yas);
	
	
	return 0;
}



#include <stdio.h>
#include <stdlib.h>


//***********************************POİNTER********************************************
//                  ÖNCE Bİ DEĞİŞKEN ATIYORUZ NORMAL İNT CHAR GİBİ----- int sayi=10
			//		SONRA İNT ATADIYSAK 								int *s = &sayi             	değer %d,sayi     adres %x,s
								//									
								
int main() {
	
	int sayi=30;
	
	int *s = &sayi;
	
	//Sayıyı yazdırma
	printf("Deger: %d\n",sayi);
	
	//Adresi yazdırma
	printf("Adres: %x\n",s);
	
	
	//Kelimeyi ekrana yazdırma
	
	char harf = 'b';
	
	char *h = &harf;
	
	printf("Kelime: %c\n", harf);
		
	// Kelimenin adresini yazdırma
	printf("Adresi: %x",h);
	
	return 0;
}
#include <stdio.h>
#include <stdlib.h>

// ************************void************************

/*
